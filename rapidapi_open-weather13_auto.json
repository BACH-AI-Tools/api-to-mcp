        "summary": "V2 - 3-hour Forecast 5 days",
        "description": "Get 5 days weather forecast data by Latitude \\u0026 Longitude",
        "operationId": "v2___3_hour_forecast_5_days",
        "parameters": [
          {
            "name": "latitude",
            "in": "query",
            "required": true,
            "description": "Example value: 40.730610",
            "schema": {
              "type": "string",
              "example": "40.730610"
            }
          },
          {
            "name": "longitude",
            "in": "query",
            "required": true,
            "description": "Example value: -73.935242",
            "schema": {
              "type": "string",
              "example": "-73.935242"
            }
          },
          {
            "name": "lang",
            "in": "query",
            "required": false,
            "description": "We support the following languages that you can use with the corresponded lang values: AFAfrikaans, ALAlbanian, ARArabic, AZAzerbaijani, BGBulgarian, CACatalan, CZCzech, DADanish, DEGerman, ELGreek, ENEnglish, EUBasque, FAPersian (Farsi), FIFinnish, FRFrench, GLGalician, HEHebrew, HIHindi, HRCroatian, HUHungarian, IDIndonesian, ITItalian, JAJapanese, KRKorean, LALatvian, LTLithuanian, MKMacedonian, NONorwegian, NLDutch, PLPolish, PTPortuguese, PT_BRPortuguês Brasil, RORomanian, RURussian, SESwed",
            "schema": {
              "type": "string",
              "example": "EN"
            }
          },
          {
            "name": "latitude",
            "in": "header",
            "required": true,
            "description": "Example value: 40.730610",
            "schema": {
              "type": "string",
              "example": "40.730610"
            }
          },
          {
            "name": "longitude",
            "in": "header",
            "required": true,
            "description": "Example value: -73.935242",
            "schema": {
              "type": "string",
              "example": "-73.935242"
            }
          },
          {
            "name": "lang",
            "in": "header",
            "required": false,
            "description": "Example value: EN",
            "schema": {
              "type": "string",
              "example": "EN"
            }
